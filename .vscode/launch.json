{
	// Use IntelliSense to learn about possible Node.js debug attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Attach by Process ID",
			"processId": "${command:PickProcess}"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch New jQuery",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"args": [
				"new"
				 ,"jqueryproj"
				 ,"--framework=jquery"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch New Angular",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"args": [
				"new"
				 ,"angularproj"
				 ,"--framework=angular"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Wizard",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch New React",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"args": [
				"new"
				 ,"reactproj"
				 ,"--framework=react"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Quickstart jQuery",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"args": [
				"quickstart",
				"--framework=jquery"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Quickstart React",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"args": [
				"quickstart",
				"--framework=react"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Quickstart Angular",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"args": [
				"quickstart",
				"--framework=angular"
			]
		},

		{
			"type": "node",
			"request": "launch",
			"name": "Launch Add",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"args": [
				"add"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceRoot}/output/proj",
			"name": "Launch Start",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"args": [
				"start"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Test",
			"program": "${workspaceFolder}/node_modules/jasmine/bin/jasmine.js",
			"preLaunchTask": "build",
			"args": [
				"spec/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Add Combo to Jquery",
			"cwd": "${workspaceRoot}/output/jqueryproj",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"args": [
				"add",
				"--template=combo",
				"--name=mycombo"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Add Combo to React",
			"cwd": "${workspaceRoot}/output/reactProj",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"args": [
				"add",
				"--template=combo",
				"--name=mycombo"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Add Combo to Angular",
			"cwd": "${workspaceRoot}/output/angularproj",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"args": [
				"add",
				"--template=combo",
				"--name=mycombo"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Wizard Reactproj",
			"cwd": "${workspaceRoot}/output/reactproj",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Generate Template",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"args": [
				"generate",
				"template"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Generate Template with name and framework",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"args": [
				"generate",
				"template",
				"--name=\"bla bla-bla\"",
				"--framework=jquery"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Generate Template with aliases g t",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"args": [
				"g",
				"t"
			],
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Generate Template with all aliases",
			"cwd": "${workspaceRoot}/output",
			"program": "${workspaceRoot}/bin/execute.js",
			"console": "externalTerminal",
			"preLaunchTask": "build",
			"args": [
				"g",
				"t",
				"-n=angular",
				"-f=angular",
				"-t=igx-ts",
				"-s=false",
			]
		}
	]
}