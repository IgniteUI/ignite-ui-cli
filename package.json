{
  "name": "ignite-ui-cli",
  "version": "1.0.0",
  "description": "Cli for installing Ignite UI templates",
  "keywords": [
    "CLI",
    "Ignite UI"
  ],
  "main": "lib/cli.js",
  "scripts": {
	"build": "node ./node_modules/typescript/lib/tsc.js",
	"pretest": "npm run lint && npm run build",
	"test": "node ./node_modules/jasmine-node/lib/jasmine-node/cli.js spec/",
	"test-debug": "node --debug-brk=5858 ./node_modules/jasmine-node/lib/jasmine-node/cli.js spec/",
	"lint": "tslint --project tsconfig.json"
  },
  "author": "PwnJS",
  "license": "ISC",
  "bin": {
    "ignite-ui": "bin/execute.js"
  },
  "dependencies": {
    "chalk": "^2.2.0",
    "fs-extra": "^3.0.1",
    "ignite-ui": "^17.1.1012",
    "inquirer": "^3.1.1",
    "lite-server": "^2.3.0",
    "resolve": "^1.3.3",
    "shelljs": "^0.7.8",
    "through2": "^2.0.3",
    "yargs": "^8.0.2",
    "yargs-promise": "^1.1.0",
	"jasmine-node": "^1.14.5",
    "typescript": "^2.5.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^3.0.3",
    "@types/inquirer": "0.0.35",
    "@types/jasmine": "^2.5.54",
    "@types/node": "^8.0.5",
    "@types/shelljs": "^0.7.4",
    "tslint": "^5.7.0"
  }
}
